record(stringin, "$(P)ERROR:STR")
{
    field(DESC, "Input error return")
	field(DTYP, "stream")
	field(SCAN, "I/O Intr")
    
	field(INP, "@keylkg.proto readErrorString $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:ERROR:STR")
    field(SDIS, "$(P)DISABLE")
    info(INTEREST, "MEDIUM")
}

